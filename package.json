{
  "name": "a11y-dashboard",
  "version": "1.0.0",
  "description": "The a11y dashboard",
  "main": "index.js",
  "scripts": {
    "bamboo": ". ./test/.env.bamboo && mocha --reporter mocha-junit-reporter",
    "build": "rimraf dist && webpack --config ./webpack.config.production.js",
    "local": "node . | bunyan",
    "dev:local": "nodemon --ignore src --exec \"npm run local\"",
    "dev:local:docker": "npm run dev:local -- -L",
    "dev:docker:boot": ". ./test/.env.local && docker-compose up",
    "dev:local:local": ". ./test/.env.local && npm run dev:local",
    "dev:prodtest": ". ./test/.env.local.prod && docker-compose up",
    "dev:local:ddev": ". ./test/.env.ddev && npm run dev:local",
    "prod:local:ddev": ". ./test/.env.ddev.prod && docker-compose rm -rf && docker-compose up",
    "lint": "eslint .",
    "postinstall": "npm run build -- --color=true",
    "start": "node .",
    "test": ". ./test/.env.local && mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/atlassian/a11y-dashboard.git"
  },
  "keywords": [
    "a11y",
    "dashboard"
  ],
  "author": "Joscha Feth <jfeth@atlassian.com>",
  "license": "Apache-2.0",
  "homepage": "https://bitbucket.org/atlassian/a11y-dashboard#readme",
  "dependencies": {
    "bunyan": "^1.7.1",
    "compression": "^1.6.1",
    "express": "^4.13.4"
  },
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "css-loader": "^0.26.1",
    "debug": "^2.2.0",
    "deep-equal": "^1.0.1",
    "es6-promise": "^4.0.5",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-react": "^6.10.0",
    "exports-loader": "^0.6.3",
    "finchjs": "^0.5.14",
    "form-serialize": "^0.7.0",
    "highlight.js": "^9.2.0",
    "history": "^4.5.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.9.0",
    "json-loader": "^0.5.4",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "lodash": "^4.6.1",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.9.1",
    "moment": "^2.11.2",
    "mustache": "^2.2.1",
    "nodemon": "^1.9.1",
    "object-assign": "^4.0.1",
    "parallelshell": "^2.0.0",
    "query-string": "^4.3.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-google-charts": "^1.4.2",
    "react-router": "^3.0.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "supertest": "^3.0.0",
    "wait-on": "^2.0.1",
    "webpack": "^2.1.0-beta",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.9.1",
    "xhr": "^2.2.0"
  }
}
