{
  "name": "a11y-dashboard",
  "version": "1.0.0",
  "description": "The a11y dashboard",
  "main": "index.js",
  "scripts": {
    "bamboo": ". ./test/.env.bamboo && mocha --reporter mocha-junit-reporter",
    "build": "rimraf dist && webpack --config ./webpack.config.production.js",
    "local": "node . | bunyan",
    "dev:local": "nodemon --ignore src --exec \"npm run local\"",
    "dev:local:docker": "npm run dev:local -- -L",
    "dev:docker:boot": ". ./test/.env.local && docker-compose up",
    "dev:local:local": ". ./test/.env.local && npm run dev:local",
    "dev:local:ddev": ". ./test/.env.ddev && npm run dev:local",
    "lint": "eslint .",
    "postinstall": "npm run build -- --colors",
    "start": "node .",
    "test": ". ./test/.env.local && mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/atlassian/a11y-dashboard.git"
  },
  "keywords": [
    "a11y",
    "dashboard"
  ],
  "author": "Joscha Feth <jfeth@atlassian.com>",
  "license": "MIT",
  "homepage": "https://bitbucket.org/atlassian/a11y-dashboard#readme",
  "dependencies": {
    "bunyan": "^1.5.1",
    "compression": "^1.6.0",
    "express": "^4.13.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "debug": "^2.2.0",
    "es6-promise": "^3.0.2",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "exports-loader": "^0.6.2",
    "finchjs": "^0.5.14",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^1.7.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.3.4",
    "mocha-junit-reporter": "^1.9.1",
    "moment": "^2.10.6",
    "mustache": "^2.2.1",
    "nodemon": "^1.8.1",
    "object-assign": "^4.0.1",
    "parallelshell": "^2.0.0",
    "query-string": "^3.0.0",
    "rimraf": "^2.4.4",
    "supertest": "^1.1.0",
    "wait-on": "^1.2.1",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "xhr": "^2.2.0"
  }
}
